{
  "name": "webpack-sandboxed",
  "version": "0.2.4",
  "description": "Webpack in a sandbox.",
  "author": "Maximilian Fellner <max.fellner@gmail.com>",
  "repository": "https://github.com/mfellner/webpack-sandboxed.git",
  "license": "MIT",
  "keywords": [
    "webpack"
  ],
  "main": "dist/index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "babel lib -d dist",
    "dist": "npm run clean && npm run build && npm run flow-gen-files",
    "lint": "eslint lib",
    "test": "npm run lint && NODE_ENV=test jest --coverage",
    "flow-check": "flow check",
    "flow-gen-files": "flow-copy-source -v lib dist",
    "flow-typed-install": "rimraf flow-typed/npm/*_vx.x.x.js && flow-typed install -o",
    "prettier": "prettier --single-quote --print-width 100 --write 'lib/**/*.js' 'test/**/*.js'",
    "prepublishOnly": "npm run dist",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "codeclimate": "codeclimate-test-reporter < ./coverage/lcov.info"
  },
  "peerDependencies": {
    "debug": "^2.2.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "enhanced-resolve": "3.1.0",
    "memory-fs": "0.4.1",
    "resolve": "1.3.3"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.0",
    "babel-loader": "7.0.0",
    "babel-plugin-syntax-flow": "6.18.0",
    "babel-plugin-transform-es2015-destructuring": "6.23.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-latest-minimal": "1.1.2",
    "babel-preset-react": "6.24.1",
    "css-loader": "0.28.1",
    "debug": "2.6.6",
    "eslint": "3.19.0",
    "eslint-plugin-babel": "4.1.1",
    "eslint-plugin-flowtype": "2.32.1",
    "eslint-plugin-prettier": "2.0.1",
    "eslint-plugin-promise": "3.5.0",
    "extract-text-webpack-plugin": "2.1.0",
    "flow-bin": "0.45.0",
    "flow-copy-source": "1.1.0",
    "flow-typed": "2.1.2",
    "jest": "20.0.0",
    "material-ui": "0.18.0",
    "prettier": "1.3.1",
    "rimraf": "2.6.1",
    "style-loader": "0.17.0",
    "webpack": "2.5.1"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "lcov",
      "text"
    ]
  }
}
