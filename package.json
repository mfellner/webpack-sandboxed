{
  "name": "webpack-sandboxed",
  "version": "0.4.0",
  "description": "Webpack in a sandbox.",
  "author": "Maximilian Fellner <max.fellner@gmail.com>",
  "repository": "https://github.com/mfellner/webpack-sandboxed.git",
  "license": "MIT",
  "keywords": [
    "webpack"
  ],
  "main": "dist/index.js",
  "engines": {
    "node": ">=7.6.0"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "tsc -p .",
    "dist": "npm run clean && npm run build",
    "lint": "tslint --type-check --project tsconfig.json && tslint test/**/*.ts",
    "test": "npm run lint && jest --coverage",
    "docs": "typedoc --readme none --tsconfig tsconfig.json --out doc",
    "prepublishOnly": "npm run dist",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "codeclimate": "codeclimate-test-reporter < ./coverage/lcov.info"
  },
  "peerDependencies": {
    "debug": "^2.6.8",
    "webpack": "^2.6.1"
  },
  "dependencies": {
    "memory-fs": "0.4.1",
    "resolve": "1.3.3"
  },
  "devDependencies": {
    "@types/debug": "0.0.29",
    "@types/extract-text-webpack-plugin": "2.1.0",
    "@types/jest": "19.2.4",
    "@types/memory-fs": "0.3.0",
    "@types/resolve": "0.0.4",
    "@types/webpack": "2.2.15",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "css-loader": "0.28.4",
    "debug": "2.6.8",
    "extract-text-webpack-plugin": "2.1.0",
    "jest": "20.0.4",
    "material-ui": "0.18.1",
    "rimraf": "2.6.1",
    "style-loader": "0.18.1",
    "ts-jest": "20.0.5",
    "ts-node": "3.0.4",
    "tslint": "5.4.2",
    "typedoc": "0.7.1",
    "typescript": "2.3.4",
    "webpack": "2.6.1"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/test/.*\\.test.ts$",
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ]
  }
}
